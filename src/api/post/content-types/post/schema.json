{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "keywords": {
      "type": "text"
    },
    "descripcion": {
      "type": "text"
    },
    "cuerpo": {
      "type": "blocks"
    },
    "bajada": {
      "type": "text"
    },
    "previsualizacion": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "imagen": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
